********* N-repeated element in size 2N array**************** //961
class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        /*unordered_map<int,int> freq;//t.c=O(n) and sc=O(n)
        int n = nums.size();
        for(int i=0; i<n; i++){
            if(freq[nums[i]]>=1){//already seen 
                return nums[i];//return the element
            }
            freq[nums[i]]++;//not seen push the element
        }
        return 0;*/
        for(int i=2; i<nums.size(); i++){//T.c=O(n) and S.c=O(1)
            if(nums[i]==nums[i-1] || nums[i]==nums[i-2]){//pigeonhole approach
                return nums[i];
            }
        }
        return nums.back();//due to 2n=4 and n=2
    }
};
