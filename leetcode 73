************SEt Matrix Zeroes**********
class Solution {
public:
    void setZeroes(vector<vector<int>>& matrix) {
        //approach--I
        //first we make a temp matrix make changes in it
        //copy the temp to original 
        int m=matrix.size();//rows
        int n=matrix[0].size();//column
        vector<vector<int>> temp=matrix;//copying the vector
        for(int i=0; i<m; i++){
            for(int j=0; j<n; j++){
                if(matrix[i][j]==0){
                    for(int k=0; k<n; k++){
                        temp[i][k]=0;//mark ith row 0
                    }
                    for(int k=0; k<m; k++){
                        temp[k][j]=0;//marking jth col 0
                    }
                }
            }
        }
        matrix=temp;
    }
};
************Maximum consecutive ones***********485
class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int n =nums.size();
        int maxcount=0;
        int count=0;
        for(int right=0; right<n; right++){
            if(nums[right]==1){
                count++;
                maxcount=max(maxcount,count);
                
            }
            else{
                count=0;
            }
        }
        return maxcount;
    }
};
