class Solution {
public:
    int missingNumber(vector<int>& nums) {
         long int n = nums.size();//value of integer
        long int total= n * (n+1)/2;//sum n interger formula
        long int sum=0;
        for(int i=0; i<nums.size(); i++){
            sum += nums[i];
        }
        return total-sum;
    }
};
