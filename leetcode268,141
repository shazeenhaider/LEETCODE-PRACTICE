class Solution {
public:
    int missingNumber(vector<int>& nums) {
         long int n = nums.size();//value of integer
        long int total= n * (n+1)/2;//sum n interger formula
        long int sum=0;
        for(int i=0; i<nums.size(); i++){
            sum += nums[i];
        }
        return total-sum;
    }
};
**************Linked List cycle***************
class Solution {
public:
    bool hasCycle(ListNode* head) {
        ListNode* slow=head;
        ListNode* fast=head;
        while(fast!=NULL && fast->next!=NULL){
            slow=slow->next;
            fast=fast->next->next;
            if(slow==fast){
                return true;
            }
        }
        return false;
    }
};
